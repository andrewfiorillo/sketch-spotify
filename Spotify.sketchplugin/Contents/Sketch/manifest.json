{
	"name": "Spotify",
	"description": "Sketch plugin to load real Spotify data into documents",
	"author": "Andrew Fiorillo",
	"homepage": "",
	"version": "1.0",
	"identifier": "com.sketch.spotify",
	"compatibleVersion": "3",
	"bundleVersion": "1",
	"commands": [
	
		{
			"name": "Top Artists",
			"identifier": "insertPhotosByTopArtists",
			"shortcut": "",
			"script": "main.js",
			"handler": "insertPhotosByTopArtists"
		},
		{
			"name": "Search by Artist...",
			"identifier": "insertArtistsPhotosByArtistSearch",
			"shortcut": "",
			"script": "main.js",
			"handler": "insertArtistsPhotosByArtistSearch"
		},
		
		
		{
			"name": "Top Albums",
			"identifier": "insertAlbumArtByTopArtists",
			"shortcut": "",
			"script": "main.js",
			"handler": "insertAlbumArtByTopArtists"
		},
		{
			"name": "New Releases",
			"identifier": "insertAlbumArtByNewReleases",
			"shortcut": "",
			"script": "main.js",
			"handler": "insertAlbumArtByNewReleases"
		},
		{
			"name": "Discover Weekly",
			"identifier": "insertAlbumArtByDiscoverWeekly",
			"shortcut": "",
			"script": "main.js",
			"handler": "insertAlbumArtByDiscoverWeekly"
		},
		{
			"name": "Search by Artist...",
			"identifier": "insertAlbumArtByArtistSearch",
			"shortcut": "",
			"script": "main.js",
			"handler": "insertAlbumArtByArtistSearch"
		},
		
		
		{
			"name": "Authenticate",
			"identifier": "authenticate",
			"shortcut": "",
			"script": "main.js",
			"handler": "authenticate"
		},
		{
			"name": "Sign In",
			"identifier": "signin",
			"shortcut": "",
			"script": "main.js",
			"handler": "signin"
		},
		{
			"name": "Sign Out",
			"identifier": "signout",
			"shortcut": "",
			"script": "main.js",
			"handler": "signout"
		},
		{
			"name": "Test",
			"identifier": "testCommand",
			"shortcut": "",
			"script": "main.js",
			"handler": "testCommand"
		},
		
		{
			"name" : "Test Action",
			"identifier": "testAction",
			"script": "main.js",
			"handlers": {
				"actions": {
					"*": "testAction"
				}
    		}
  		}
  		
	],
	
	"menu": {
		"items": [
			{
				"title": "Artist Photos",
				"items": [
					"insertPhotosByTopArtists",
					"insertArtistsPhotosByArtistSearch"
				]
			},
			{
				"title": "Album Art",
				"items": [
					"insertAlbumArtByTopArtists",
					"insertAlbumArtByNewReleases",
					"insertAlbumArtByDiscoverWeekly",
					"insertAlbumArtByArtistSearch"
				]
			},
			"-",
			"signin",
			"signout"
		]
		
	}
}